{
  "name": "@salesforce/plugin-devops-center",
  "description": "The DevOps Center CLI plugin provides a command-line alternative to performing equivalent actions in the DevOps Center graphical UI",
  "version": "1.0.0-beta.1",
  "author": "Salesforce",
  "bugs": "https://github.com/forcedotcom/cli/issues",
  "dependencies": {
    "@oclif/core": "^2.4.0",
    "@salesforce/core": "^3.34.4",
    "@salesforce/kit": "^1.8.2",
    "@salesforce/sf-plugins-core": "^2.2.5",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^3.3.9",
    "@oclif/test": "^2.3.7",
    "@salesforce/cli-plugins-testkit": "^3.2.25",
    "@salesforce/dev-config": "^3.1.0",
    "@salesforce/dev-scripts": "^3.1.1",
    "@salesforce/plugin-command-reference": "^2.3.1",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "1.4.6",
    "@swc/cli": "^0.1.62",
    "@swc/core": "1.3.39",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.1",
    "chai": "^4.3.7",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-salesforce": "^1.1.1",
    "eslint-config-salesforce-license": "^0.2.0",
    "eslint-config-salesforce-typescript": "^1.1.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-sf-plugin": "^1.14.0",
    "husky": "^7.0.4",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "oclif": "^3.7.0",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "shx": "0.3.4",
    "sinon": "10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json",
    "/schemas"
  ],
  "homepage": "https://github.com/salesforcecli/plugin-devops-center",
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-plugin",
    "sf-plugin",
    "sf"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sf",
    "configMeta": "./lib/configMeta",
    "topicSeparator": " ",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference"
    ],
    "topics": {
      "project": {
        "external": true,
        "subtopics": {
          "deploy": {
            "description": "Commands to deploy artifacts to an environment.",
            "subtopics": {
              "pipeline": {
                "description": "Commands to deploy metadata from a pipeline stage to a Salesforce org."
              }
            }
          }
        }
      }
    }
  },
  "repository": "salesforcecli/plugin-devops-center",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint && yarn test:deprecation-policy && yarn test:json-schema && yarn test:command-reference",
    "prepack": "sf-prepack",
    "prepare": "sf-install",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:command-reference": "./bin/dev commandreference:generate --erroronwarnings",
    "test:deprecation-policy": "./bin/dev snapshot:compare",
    "test:json-schema": "./bin/dev schema:compare",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000 --parallel",
    "version": "oclif readme"
  },
  "publishConfig": {
    "access": "public"
  }
}
